	private void updatePlayerList() {
		for (int i = -1; i < World.getWorld().getPlayerList().size(); i++) {
			int j = 0;
			if (i == -1) {
				// j = this.myPlayerIndex;
			} else {
				// j = this.playerIndices[i];
			}
			Player player = World.getWorld().getPlayerList().get(j);
			if (player != null) {
				this.entityUpdateBlock(player);
			}
		}
	}